{
    "eula": "",
    "vendor": "SAP",
    "license": "",
    "id": "com.sap.sample.sacjavascripthelper",
    "version": "0.0.1",
    "name": "JavaScript Helper for SAC",
    "newInstancePrefix": "SACJavaScriptHelper",
    "description": "A widget that wraps common JS library objects",
	"icon": "",
    "imports": ["datasource", "input-controls"],
    "webcomponents": [
      {
        "kind": "main",
        "tag": "com-sap-sample-sacjavascripthelper",
        "url": "https://xpagcoe.github.io/CustomWidgets/SAC%20JavaScriptHelper/sacjavascripthelper.js",
        "integrity": "",
        "ignoreIntegrity": true
      }
    ],
    "properties": {
		"metadata": {
			"type": "string",
			"description": "For internal use",
			"default": ""
		}
    },
    "methods": {
		"createMap": {
			"returnType": "boolean",
			"description": "Create a new Map object and return it as JSON string",
			"parameters": [ {"name": "map_id", "type": "string", "description": "Map object unique id"} ]
		},
		"setMapItem": {
			"returnType": "boolean",
			"description": "Set a key-value pair",
			"parameters": [ {"name": "map_id", "type": "string", "description": "Map object unique id"},
							{"name": "key", "type": "string", "description": "Key index"},
							{"name": "value", "type": "Selection", "description": "Value as JSON string"} ]
		},
		"getMapItem": {
			"returnType": "Selection",
			"description": "Get a value from key",
			"parameters": [ {"name": "map_id", "type": "string", "description": "Map object unique id"},
							{"name": "key", "type": "string", "description": "Key index"} ]
		}
    },
    "events": {
    }
}